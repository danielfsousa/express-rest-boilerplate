{
  "name": "express-rest-boilerplate",
  "version": "0.0.0",
  "private": true,
  "description": "Express boilerplate for building RESTful APIs",
  "repository": {
    "type": "git",
    "url": "git@github.com:danielfsousa/express-rest-boilerplate.git"
  },
  "license": "MIT",
  "author": {
    "name": "Daniel Sousa",
    "email": "sousa.dfs@gmail.com"
  },
  "type": "module",
  "imports": {
    "#config": "./src/config.js",
    "#controllers/*": "./src/controllers/*.controller.js",
    "#enums/*": "./src/enums/*.enum.js",
    "#errors/*": "./src/errors/*.error.js",
    "#lib/*": "./src/lib/*.js",
    "#middlewares/*": "./src/middlewares/*.middleware.js",
    "#models/*": "./src/models/*.model.js",
    "#routes/*": "./src/routes/*.route.js",
    "#services/*": "./src/services/*.service.js",
    "#validations/*": "./src/validations/*.validation.js"
  },
  "main": "src/main.js",
  "scripts": {
    "dev": "nodemon ./src/main.js",
    "docker:down": "docker compose down",
    "docker:up": "docker compose up",
    "lint": "eslint . ; prettier . --check",
    "lint:fix": "eslint . --fix ; prettier --write .",
    "prepare": "husky install",
    "start": "node ./src/main.js",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "validate": "lint-staged"
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.{js,json,md,yaml,yml,pug}": "prettier --write"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.9.0",
    "bcryptjs": "2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.3",
    "date-fns": "^2.29.3",
    "dotenv": "^10.0.0",
    "email-templates": "^8.0.7",
    "envalid": "^7.2.1",
    "express": "^4.15.2",
    "express-async-errors": "^3.1.1",
    "express-pino-logger": "^6.0.0",
    "got": "^12.5.2",
    "helmet": "^4.6.0",
    "http-status": "^1.5.0",
    "jwt-simple": "0.5.6",
    "lodash-es": "^4.17.21",
    "mongoose": "^5.2.17",
    "nodemailer": "^6.3.1",
    "openapi-validator-middleware": "^3.2.3",
    "passport": "^0.6.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "4.0.0",
    "pino": "^6.13.0",
    "pug": "^3.0.1",
    "uuid": "^8.3.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@danielfsousa/prettier-config": "^1.1.0",
    "@prettier/plugin-pug": "^1.16.4",
    "@trivago/prettier-plugin-sort-imports": "^2.0.2",
    "@vitest/coverage-c8": "^0.24.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.2.3",
    "nodemon": "^2.0.1",
    "open-cli": "^7.0.0",
    "pino-pretty": "^5.1.3",
    "prettier": "^2.3.2",
    "prettier-plugin-packagejson": "^2.2.13",
    "supertest": "^6.1.3",
    "swagger-ui-express": "^4.1.6",
    "testcontainers": "^9.0.0",
    "vitest": "^0.23.4"
  },
  "engines": {
    "node": ">=18"
  }
}
